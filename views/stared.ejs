<% include common/header %>
    <div class="container">
      <div class="row">
        <div class="col s3">
          <% include common/aside %>
        </div>

        <div class="col s9">
          <div class="collection z-depth-2">
            <% staredList.forEach(function (todo) { %>
              <div class="collection-item">
                <div class="row">
                  <p class="left col s10">
                    <input type="checkbox" id="<%= todo._id %>" />
                    <label for="<%= todo._id %>" style="color:#333"><%= todo.content %></label>
                  </p>
                  <h6 class="left" style="margin-left:44px"><%= todo.deadline%></h6>
                    <a href="#" class="right col s1"><i class="material-icons">delete</i></a>
                    <a href="#" class="right col s1"><i class="material-icons">
                  <% if(todo.stared) {%>
                    star
                  <%} else { %>
                    star_border
                  <%}%></i></a>
                </div>
              </div>
            <% }) %>


          </div>
        </div>

      </div>
      <div class="fixed-action-btn" style="bottom: 45px; right: 24px;">
        <a class="btn-floating btn-large waves-effect waves-light red" href="/add">
          <i class="material-icons">add</i>
        </a>
    </div>
    <script type="text/javascript" src="https://code.jquery.com/jquery-2.1.1.min.js"></script>
    <script type="text/javascript" src="javascripts/materialize.min.js"></script>
    <script type="text/javascript">
      $('.datepicker').pickadate({
        selectMonths: true, // Creates a dropdown to control month
        selectYears: 15 // Creates a dropdown of 15 years to control year
      });
    </script>
  </body>
</html>
